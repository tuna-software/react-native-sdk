{
  "name": "@tuna-software/react-native-sdk",
  "version": "1.4.0",
  "description": "React Native payment processing SDK for Tuna with native Apple Pay and Google Pay support",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "react-native": "src/index.ts",
  "source": "src/index.ts",
  "files": [
    "src",
    "example",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "!src/**/__tests__",
    "!src/**/__mocks__",
    "!example/TunaPaymentDemo/android/app/build",
    "!example/**/node_modules"
  ],
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "example": "cd example/TunaPaymentDemo && npx expo start"
  },
  "keywords": [
    "react-native",
    "payments",
    "apple-pay",
    "google-pay",
    "tuna",
    "gateway",
    "credit-card",
    "tokenization",
    "pci",
    "mobile-payments"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tuna-software/react-native-sdk.git"
  },
  "author": "Tuna Software Inc <tech@tuna.uy>",
  "homepage": "https://tuna.uy",
  "bugs": {
    "url": "https://github.com/tuna-software/react-native-sdk/issues"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@rnw-community/react-native-payments": "^1.6.0",
    "react": ">=17.0.0",
    "react-native": ">=0.64.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@babel/preset-typescript": "^7.22.0",
    "@rollup/plugin-babel": "^6.0.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.4.0",
    "@types/react": "^18.2.0",
    "@types/react-native": "^0.72.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "react": "^18.2.0",
    "react-native": "^0.72.0",
    "rollup": "^3.26.0",
    "rollup-plugin-dts": "^5.3.0",
    "typescript": "^5.1.0"
  },
  "dependencies": {
    "card-validator": "^8.1.1",
    "react-dom": "^19.1.0",
    "react-native-web": "^0.21.1",
    "react-native-webview": "^13.16.0"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_"
        }
      ]
    }
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 2
  }
}
